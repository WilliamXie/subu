            nodes =3D Node.findAll(Node.class, "parentId=3D?", new =
Object[]{id});
        }
=20
        List&lt;Map&lt;String,Object&gt;&gt; items =3D new =
ArrayList&lt;Map&lt;String,Object&gt;&gt;();
        for(Node node: nodes){
            Map&lt;String,Object&gt; item =3D new =
HashMap&lt;String,Object&gt;();
            item.put("id", node.id);
            item.put("text", node.name);
=20
            // the node has children,=20
            // set the state to 'closed' so the node can asynchronous =
load children nodes=20
            if (node.hasChildren()){
